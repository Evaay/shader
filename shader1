#ifdef GL_ES
precision mediump float;
#endif
uniform vec2 u_resolution;
uniform float u_time;
void main(){
vec2 s=gl_FragCoord.xy/u_resolution-.5,c=s-vec2(0,.05);
float d=max(abs(c.x),abs(c.y)),r=fract(d*12.),i=floor(d*12.);
gl_FragColor=vec4(d<.5?mix(vec3(0.,.6,1.),vec3(0.,1.-i*.05,.2),step(r,.4)):vec3(0.,.6,1.),1.);
}
